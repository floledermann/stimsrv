{
  "name": "stimsrv",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-server": "nodemon --ignore static --ignore data -e js,json,html server.js ./experiment-simple.js",
    "watch-client": "nodemon --ignore static --ignore data -e js --exec \"npm run build-client\"",
    "watch-experiment": "nodemon --ignore static --ignore data -e js --exec \"npm run build-experiment\"",
    "debug-server": "node --inspect-brk server.js ./experiment-simple.js",
    "build-client": "browserify -r ./src/clients/browser/client-browser.js:stimsrvClient -s stimsrvClient -o static/client-browser.js",
    "build-experiment": "browserify -r ./experiment-simple.js:experiment -s experiment -o static/experiment.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/floledermann/stimsrv.git"
  },
  "author": "Florian Ledermann <florian.ledermann@tuwien.ac.at>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/floledermann/stimsrv/issues"
  },
  "homepage": "https://github.com/floledermann/stimsrv#readme",
  "dependencies": {
    "another-dimension": "0.2.3",
    "body-parser": "1.19.0",
    "browserify": "17.0.0",
    "cookie-parser": "1.4.5",
    "d3-interpolate": "2.0.1",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "hashids": "2.2.8",
    "mri": "1.1.6",
    "nunjucks": "3.2.2",
    "socket.io": "3.1.0",
    "socket.io-client": "3.1.1"
  },
  "devDependencies": {
    "nodemon": "2.0.7"
  }
}
